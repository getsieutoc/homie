{
  "name": "homie",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf node_modules dist .next && pnpm install",
    "dev": "docker compose up -d && pnpm run watch && docker compose stop",
    "watch": "trap 'exit 0' SIGINT; concurrently --raw --kill-others pnpm:dev:*",
    "dev:trigger": "pnpm dlx trigger.dev@latest dev",
    "dev:prisma": "prisma generate --watch",
    "dev:next": "next dev --turbo",
    "ui:add": "pnpm dlx shadcn@latest add",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write",
    "prisma": "prisma",
    "prepare": "husky",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
